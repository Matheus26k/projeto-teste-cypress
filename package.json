{
  "name": "projeto-teste-cypress",
  "version": "1.0.0",
  "description": "Projeto de Teste Automatizado com Cypress",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "run:chrome": "cypress run --browser chrome",
    "run:firefox": "cypress run --browser firefox",
    "atualizar-git": "git add . && git commit -m \"Atualização automática\" && git push",
    "lint": "eslint . --ext .js",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "cypress": "^14.3.2",
    "cypress-mochawesome-reporter": "^3.8.2",
    "eslint": "^8.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "prettier": "^2.8.8"
  }
}
